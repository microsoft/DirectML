{
    "$schema": "./_schema.json",
    
    "resources": 
    {
        "a": 
        {
            "initialValuesDataType": "FLOAT32",
            "initialValues": [ 1, 2, 3, 4 ]
        },
        "b": 
        {
            "initialValuesDataType": "FLOAT32",
            "initialValues": [ 5, 6, 7, 8 ]
        },
        "c": 
        {
            "initialValuesDataType": "FLOAT32",
            "initialValues": { "valueCount": 8, "value": 0 }
        }
    },

    "dispatchables": 
    {
        "concat": 
        {
            "type": "onnx",
            "sourcePath": "onnx_concat.onnx"
        }
    },

    "commands": 
    [
        {
            "type": "dispatch",
            "dispatchable": "concat",
            "bindings": 
            {
                "x0": { "name": "a", "shape": [2,2] },
                "x1": { "name": "b", "shape": [2,2] },
                "y": { "name": "c", "shape": [4,2] }
            }
        }
        ,
        {
            "type": "print",
            "resource": "c"
        }
    ]
}